<h1>Listing journals</h1>

<table>
  <tr>
    <th>User</th>
    <th>Title</th>
    <th>Note</th>
    <th>Start time</th>
    <th>End time</th>
    <th>Is current</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @journals.each do |journal| %>
  <tr>
    <td><%= journal.user_id %></td>
    <td><%= journal.title %></td>
    <td><%= journal.note %></td>
    <td><%= journal.start_time %></td>
    <td><%= journal.end_time %></td>
    <td><%= journal.is_current %></td>
    <td><%= link_to 'Show', journal %></td>
    <td><%= link_to 'Edit', edit_journal_path(journal) %></td>
    <td><%= link_to 'Destroy', journal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Journal', new_journal_path %>
