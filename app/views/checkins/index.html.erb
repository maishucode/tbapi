<h1>Listing checkins</h1>

<table>
  <tr>
    <th>Journal</th>
    <th>Title</th>
    <th>Note</th>
    <th>Checkin time</th>
    <th>Longitude</th>
    <th>Latitude</th>
    <th>Poi</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @checkins.each do |checkin| %>
  <tr>
    <td><%= checkin.journal_id %></td>
    <td><%= checkin.title %></td>
    <td><%= checkin.note %></td>
    <td><%= checkin.checkin_time %></td>
    <td><%= checkin.longitude %></td>
    <td><%= checkin.latitude %></td>
    <td><%= checkin.poi %></td>
    <td><%= link_to 'Show', checkin %></td>
    <td><%= link_to 'Edit', edit_checkin_path(checkin) %></td>
    <td><%= link_to 'Destroy', checkin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Checkin', new_checkin_path %>
